{% for block in section.blocks %}
{% endfor %}

{% schema %}
{
  "name": "Table Display",
  "settings": [
    {
      "type": "header",
      "content": "Tailwind Styling Classes"
    },
    {
      "type": "textarea",
      "id": "timeline__main_section",
      "label": "Main Section Styling",
      "default": "bg-blue-100 p-5"
    }
  ],
  "blocks": [
    {
      "type": "timeline_block",
